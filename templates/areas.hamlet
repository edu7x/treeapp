    <body class="text-center">
        <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
            <header class="masthead mb-auto">
                <div class="inner">
                        <h3 class="masthead-brand">Início
                        <nav class="nav nav-masthead justify-content-center">
                            <a class="nav-link active" href=@{HomeR}>Início
                            <a class="nav-link" href=@{EspeciesCadastrarR}>Cadastrar Especies
                            <a class="nav-link" href=@{EspeciesR}>Especies
                            <a class="nav-link" href=@{EquipamentosCadastrarR}>Cadastrar Equipamentos
                            <a class="nav-link" href=@{EquipamentosR}>Equipamentos
                            <a class="nav-link" href=@{AreasCadastrarR}>Cadastrar Areas

<table class="table table-striped">
    <thead>
        <tr>
            <th>
                Descrição
            <th>
                Latitude Ponto 1
            <th>
                Longitude Ponto 1
            <th>
                Latitude Ponto 2
            <th>
                Longitude Ponto 2
            <th>
                Latitude Ponto 3
            <th>
                Longitude Ponto 3
            <th>
                Deletar?
    <tbody>
        $forall (Entity area_id area) <- areas
            <tr>
                <td>
                    #{areasDescricao area}
                <td>
                    #{areasLatitude1 area}
                <td>
                    #{areasLongitude1 area}
                <td>
                    #{areasLatitude2 area}
                <td>
                    #{areasLongitude2 area}
                <td>
                    #{areasLatitude3 area}
                <td>
                    #{areasLongitude3 area}
                <td>
                    <form action=@{AreasDeletarR area_id} method=post>
                        <input type="submit" value="X">